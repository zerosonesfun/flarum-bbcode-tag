{
    "name": "zerosonesfun/flarum-bbcode-tag",
    "description": "Keyword, hashtag, cashtag, Google, and Twitter search words/phrases.",
    "type": "flarum-extension",
    "keywords": ["discussion", "bbcode"],
    "license": "MIT",
    "authors": [
        {
            "name": "Billy Wilcosky",
            "homepage": "https://www.wilcosky.com",
            "role": "Creator"
        }
    ],
    "support": {
        "issues": "https://github.com/zerosonesfun/flarum-bbcode-tag/issues",
        "source": "https://github.com/zerosonesfun/flarum-bbcode-tag",
        "forum": "https://discuss.flarum.org"
    },
    "homepage": "https://www.wilcosky.com",
    "funding": [
        {
            "type": "website",
            "url": "https://www.wilcosky.com"
        }
    ],
    "require": {
        "flarum/core": "^0.1.0-beta.16 || ^1.0"
    },
    "autoload": {
        "psr-4": {
            "ZerosOnesFun\\BbcodeTag\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "BBCode - Tag",
            "category": "feature",
            "icon": {
                "name": "fas fa-search",
                "backgroundColor": "#ddd",
                "color": "#000"
            }
        }
    }
}
